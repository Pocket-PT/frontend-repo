{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "components/*": ["./src/components/*"],
      "pages/*": ["./src/pages/*"],
      "styles/*": ["./src/styles/*"],
      "utils/*": ["./src/utils/*"],
      "public/*": ["./public/*"],
      "hooks/*": ["./src/hooks/*"],
      "types/*": ["./src/types/*"],
      "images/*": ["./src/images/*"],
      "apps/*": ["./src/apps/*"],
      "icons/*": ["./src/icons/*"],
      "constants/*": ["./src/constants/*"],
      "videos/*": ["./src/videos/*"],
      "libs/*": ["./src/libs/*"],
      "stores/*": ["./src/stores/*"],
      "apis/*": ["./src/apis/*"]
    }
  },
  "include": [
    "./src/**/*",
    "./gatsby-node.ts",
    "./gatsby-config.ts",
    "./plugins/**/*",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "public",
    ".cache",
    "dist",
    "gatsby-config.js",
    "gatsby-node.js",
    "gatsby-ssr.js",
    "gatsby-browser.js"
  ]
}
